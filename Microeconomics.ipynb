    "        <td>$0</td>\n",
    "        <td>Mild</td>\n",
    "    </tr>\n",
    "    <tr>\n",
    "        <td>2</td>\n",
    "        <td>$100k</td>\n",
    "        <td>None</td>\n",
    "    </tr>\n",
    "</table>\n",
    "        </td>\n",
    "        <td>\n",
    "<b>Exercise 2</b>\n",
    "<table border=\"1\">\n",
    "    <tr>\n",
